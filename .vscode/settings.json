{
	"editor.insertSpaces": false,
	"editor.wordWrap": "on",
	"editor.autoClosingOvertype": "auto",
	"editor.smoothScrolling": true,
	"editor.tabCompletion": "onlySnippets",
	"editor.maxTokenizationLineLength": 200000,
	"editor.suggest.filterGraceful": true,
	"editor.tabSize": 4,
	"workbench.editor.highlightModifiedTabs": true,
	"code-runner.runInTerminal": true,
	"code-runner.clearPreviousOutput": true,
	"sftp.debug": true,
	"sftp.downloadWhenOpenInRemoteExplorer": true,
	"sftp.printDebugLog": true,
	"git.autofetch": true,
	"git.enabled": true,
	"html-css-class-completion.enableEmmetSupport": true,
	"emmet.triggerExpansionOnTab": true,
	"editor.renderControlCharacters": true,
	"editor.accessibilitySupport": "off",
	"editor.suggest.snippetsPreventQuickSuggestions": false,
	"cssPeek.trace.server": "messages",
	"git.confirmSync": false,
	"editor.fontFamily": "Cascadia Code PL, 'Courier New', monospace",
	"editor.find.cursorMoveOnType": true,
	"prettier.tabWidth": 4,
	"prettier.useTabs": true,
	"[vue]": {
		"editor.defaultFormatter": "Wscats.vue"
	},
	"prettier.vueIndentScriptAndStyle": true,
	"emmet.includeLanguages": {
		"vue-html": "html",
		"php": "html"
	},
	"path-intellisense.showHiddenFiles": true,
	"path-intellisense.absolutePathToWorkspace": true,
	"editor.fontLigatures": "'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'zero', 'onum'",
	"terminal.integrated.fontSize": 13,
	"workbench.iconTheme": "material-icon-theme",
	"workbench.editor.enablePreview": false,
	"files.associations": {
		"*.json": "jsonc"
	},
	"code-runner.executorMap": {
		"javascript": "node",
		"java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
		"c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
		"cpp": "cd $dir && g++ $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
		"objective-c": "cd $dir && gcc -framework Cocoa $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
		"php": "php",
		"python": "python -u",
		"perl": "perl",
		"perl6": "perl6",
		"ruby": "ruby",
		"go": "go run",
		"lua": "lua",
		"groovy": "groovy",
		"powershell": "powershell -ExecutionPolicy ByPass -File",
		"bat": "cmd /c",
		"shellscript": "bash",
		"fsharp": "fsi",
		"csharp": "cd $dir && dotnet run $fileName",
		"vbscript": "cscript //Nologo",
		"typescript": "ts-node",
		"coffeescript": "coffee",
		"scala": "scala",
		"swift": "swift",
		"julia": "julia",
		"crystal": "crystal",
		"ocaml": "ocaml",
		"r": "Rscript",
		"applescript": "osascript",
		"clojure": "lein exec",
		"haxe": "haxe --cwd $dirWithoutTrailingSlash --run $fileNameWithoutExt",
		"rust": "cd $dir && rustc $fileName && $dir$fileNameWithoutExt",
		"racket": "racket",
		"scheme": "csi -script",
		"ahk": "autohotkey",
		"autoit": "autoit3",
		"dart": "dart",
		"pascal": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
		"d": "cd $dir && dmd $fileName && $dir$fileNameWithoutExt",
		"haskell": "runhaskell",
		"nim": "nim compile --verbosity:0 --hints:off --run",
		"lisp": "sbcl --script",
		"kit": "kitc --run",
		"v": "v run",
		"sass": "sass --style expanded",
		"scss": "scss --style expanded",
		"less": "cd $dir && lessc $fileName $fileNameWithoutExt.css",
		"FortranFreeForm": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
		"fortran-modern": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
		"fortran_fixed-form": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
		"fortran": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt"
	},
	"code-runner.saveAllFilesBeforeRun": true,
	"code-runner.saveFileBeforeRun": true,
	"terminal.integrated.cursorStyle": "line",
	"editor.minimap.enabled": false,
	"files.exclude": {
		"**/.git": false
	},
	"explorer.confirmDelete": false,
	"workbench.list.smoothScrolling": true,
	"editor.cursorSmoothCaretAnimation": true,
	"workbench.colorTheme": "Codebumble Dark Pro",
	"editor.inlineSuggest.enabled": true,
	"github.copilot.enable": {
		"*": true,
		"yaml": false,
		"plaintext": false,
		"markdown": false,
		"scss": false
	},
	"terminal.integrated.enableBell": true,
	"terminal.integrated.profiles.windows": {
		"PowerShell": {
			"source": "PowerShell",
			"args": ["-nolog"]
		}
	},
	"terminal.integrated.defaultProfile.windows": "PowerShell",
	"editor.formatOnType": false,
	"editor.formatOnSave": true,
	"files.autoSave": "afterDelay",
	"files.autoSaveDelay": 1000,
	"git.enableSmartCommit": true,
	"workbench.editor.untitled.hint": "hidden",
	"php-docblocker.alignParams": true,
	"php-docblocker.alignReturn": true,
	"files.trimTrailingWhitespace": true,
	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[html]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"terminal.integrated.fontFamily": "Cascadia Code PL",
}
